#import <Foundation/Foundation.h>


@interface InviteMsgUserInfo : NSObject {
	NSString	*RPKey;
	NSString	*ID;
	NSString	*nickName;
	NSString	*formatted;
	NSString	*photoUrl;
	NSString	*status;	// 오늘의 한마디
	NSString	*groupId;	// 그룹 아이디
	NSString	*phoneNum;	// 핸드폰 번호
	NSString	*imStatus;	// -1:주소록 타입  0:offline  1:online  2:busy (바빠요)  8:곧 돌아옵니다.  16:다른용무중
	BOOL		checked;
}

@property (nonatomic, retain) NSString *RPKey;
@property (nonatomic, retain) NSString *ID;
@property (nonatomic, retain) NSString *nickName;
@property (nonatomic, retain) NSString *formatted;
@property (nonatomic, retain) NSString *photoUrl;
@property (nonatomic, retain) NSString *status;
@property (nonatomic, retain) NSString *groupId;
@property (nonatomic, retain) NSString *phoneNum;
@property (nonatomic, retain) NSString *imStatus;
@property (nonatomic) BOOL checked;

@end
